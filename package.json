{
  "name": "lodestar-router",
  "version": "1.0.0-pre-release",
  "description": "A simple but powerful router, it will be used in the LodestarJs framework.",
  "main": "src/main.js",
  "devDependencies": {
    "babel-core": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-rollup": "^1.0.0",
    "chai": "^3.4.1",
    "codecov.io": "^0.1.6",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^1.10.3",
    "gobble": "^0.10.2",
    "gobble-babel": "^5.5.8",
    "gobble-cli": "^0.6.0",
    "gobble-eslint": "^0.1.0",
    "gobble-rollup": "^0.11.0",
    "gobble-rollup-babel": "^0.6.1",
    "gobble-uglifyjs": "^0.2.1",
    "karma": "^0.13.15",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-browserify": "^4.4.2",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-rollup-preprocessor": "^2.0.0",
    "mocha": "^2.3.4",
    "phantomjs": "^1.9.19",
    "rollup": "^0.21.2",
    "rollup-plugin-babel": "^2.2.0"
  },
  "scripts": {
    "build": "rm -r dist && ./node_modules/.bin/gobble build dist",
    "test": "./node_modules/.bin/karma start",
    "lint": "node node_modules/eslint/bin/eslint src/",
    "coverage": "NODE_ENV=tests ./node_modules/babel-cli/bin/babel-node.js ./node_modules/.bin/babel-istanbul cover ./node_modules/mocha/bin/_mocha",
    "codecov": "rm -r coverage && npm run coverage && cat ./coverage/lcov/lcov.info | ./node_modules/.bin/codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lodestarjs/lodestar-router.git"
  },
  "author": "danjford <github.com/danjford>",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lodestarjs/lodestar-router/issues"
  },
  "homepage": "https://github.com/lodestarjs/lodestar-router#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
